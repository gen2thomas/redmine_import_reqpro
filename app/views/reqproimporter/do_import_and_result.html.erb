<% content_for :header_tags do %>
    <%= stylesheet_link_tag 'reqproimporter', :plugin => 'redmine_import_reqpro' %>
<% end %>

<h2><%= l(:label_reqproimport_result) %></h2>
<%= render :partial => "progress_bar" %><br/>
<%= l(:label_conclution_line1) %><br/>
<br/>
<table class="tab_center">
	<thead><tr>
		<th>&nbsp;</th>
	    <% @imp_res_header.each do |a_header| %>
	      <th><%= l_or_humanize(a_header.to_s, :prefix => "label_header_") %> (<%= @imp_res[:sum][a_header] %>)</th>
	    <% end %>
	</tr></thead>
	<% @imp_res_first_column.each do |result_key| %>
	  <% result_content = @imp_res[result_key] %>
	  <tbody>
	    <tr class="<%= cycle("odd", "even") %>">
	      <% if result_content[:failed] != 0 then %>
	        <th class="th_firstcolumn_failed"><%= l("#{"label_header_"}#{result_key.to_s}", :icount => @imp_res[result_key][:sum]) %></th>
	      <% else %>
	        <th class="th_firstcolumn"><%= l("#{"label_header_"}#{result_key.to_s}", :icount => @imp_res[result_key][:sum]) %></th>
	      <% end %>
	      <% @imp_res_header.each do |row| %><% column=result_content[row] %>
	        <% if column != 0 and row == :failed then %><td class="td_content_failed"><%= column %></td><% else %><td class="td_content"><%= column %></td><% end %>
	      <% end %>
	    </tr>
	  </tbody>
	<% end %>
</table>
<br/>
<hr/>
<%= l(:label_internalrelations_failed_line1) %><br/>


